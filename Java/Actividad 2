public class MyClass {
    public static void main(String args[]) {
        
     Aprendiz cris = new Aprendiz(1129499343,"cristian12@gmail.com");
    
     cris.dis = 1;
     cris.edad = 17; 
     
      System.out.println(cris.id);
      System.out.println(cris.email);
      System.out.println(cris.dis);
      System.out.println(cris.edad);
      System.out.println();
      
     Aprendiz juan = new Aprendiz(1129499343,"juan112@gmail.com");
    
     juan.dis = 1;
     juan.edad = 17; 
     
      System.out.println(juan.id);
      System.out.println(juan.email);
      System.out.println(juan.dis);
      System.out.println(juan.edad);
      System.out.println();
      
      Aprendiz andres = new Aprendiz(1129499343,"andres12@gmail.com");
    
     andres.dis = 1;
     andres.edad = 17; 
     
      System.out.println(andres.id);
      System.out.println(andres.email);
      System.out.println(andres.dis);
      System.out.println(andres.edad);
      System.out.println();
      
      Aprendiz alan = new Aprendiz(1129499343,"alan12@gmail.com");
    
     alan.dis = 1;
